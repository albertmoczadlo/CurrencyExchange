    <div class="rates-item-holder" data-rate-item="EUR_PLN">
        <div class="rates-item" data-seo-link="/kurs-euro/">
            <div class="rates-item__header">
                <div class="rates-item__header-icon">
                    <div class="icon-currency-round-EUR icon-currency-round-EUR-dims"></div>
                </div>
                <a href="/kurs-euro/" class="rates-item__header-text">
                    Euro Rate
                </a>
            </div>
