                    <a href="/kurs-euro/" class="rates-item__short-name-text">
                        EUR
                    </a>
                </div>
                <div class="rates-item__we-buy">
                    <div class="rates-item__rate-number-text" data-js-selector="buy-rate">@(ViewData["EUR_buy"])</div>
                    <div class="rates-item__rate-number-subtext">Buy At</div>
                </div>
                <div class="rates-item__we-sell">
